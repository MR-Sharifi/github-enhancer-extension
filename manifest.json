{
    "name": "Github Crawler",
    "description": "This extentsion crawl github repositories and get additional information from them.!",
    "version": "0.1",
    "manifest_version": 2,

    "icons": {
        "48": "icons/crawler-48.png",
        "225": "icons/crawler-225.png"
    },

    "content_scripts": [
        {
          "matches": ["https://github.com/*/network/members"],
          "js": ["forks-crawler.js"]
        }
    ]
}